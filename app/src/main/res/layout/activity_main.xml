<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:focusableInTouchMode="true"
    android:background="#210D08">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2"
        android:id="@+id/button"
        android:width="70dp"
        android:minWidth="5dp"
        android:height="70dp"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:onClick="addNumber"
        android:tag="2"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:id="@+id/button2"
        android:width="70dp"
        android:minWidth="5dp"
        android:height="70dp"
        android:layout_alignTop="@+id/button"
        android:layout_toLeftOf="@+id/button"
        android:layout_toStartOf="@+id/button"
        android:onClick="addNumber"
        android:tag="1" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="3"
        android:id="@+id/button3"
        android:width="70dp"
        android:minWidth="5dp"
        android:height="70dp"
        android:layout_alignTop="@+id/button"
        android:layout_toRightOf="@+id/button"
        android:layout_toEndOf="@+id/button"
        android:onClick="addNumber"
        android:tag="3" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="5"
        android:id="@+id/button4"
        android:width="70dp"
        android:minWidth="5dp"
        android:height="70dp"
        android:layout_below="@+id/button"
        android:layout_centerHorizontal="true"
        android:onClick="addNumber"
        android:tag="5" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="6"
        android:id="@+id/button5"
        android:width="70dp"
        android:minWidth="5dp"
        android:height="70dp"
        android:layout_below="@+id/button"
        android:layout_toRightOf="@+id/button4"
        android:layout_toEndOf="@+id/button4"
        android:onClick="addNumber"
        android:tag="6"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="4"
        android:id="@+id/button6"
        android:width="70dp"
        android:minWidth="5dp"
        android:height="70dp"
        android:layout_below="@+id/button2"
        android:layout_alignLeft="@+id/button2"
        android:layout_alignStart="@+id/button2"
        android:onClick="addNumber"
        android:tag="4" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="8"
        android:id="@+id/button7"
        android:width="70dp"
        android:minWidth="5dp"
        android:height="70dp"
        android:layout_below="@+id/button4"
        android:layout_alignRight="@+id/button4"
        android:layout_alignEnd="@+id/button4"
        android:onClick="addNumber"
        android:tag="8" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="7"
        android:id="@+id/button8"
        android:width="70dp"
        android:minWidth="5dp"
        android:height="70dp"
        android:layout_below="@+id/button6"
        android:layout_alignLeft="@+id/button6"
        android:layout_alignStart="@+id/button6"
        android:onClick="addNumber"
        android:tag="7" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="9"
        android:id="@+id/button9"
        android:width="70dp"
        android:minWidth="5dp"
        android:height="70dp"
        android:layout_below="@+id/button4"
        android:layout_toRightOf="@+id/button4"
        android:layout_toEndOf="@+id/button4"
        android:onClick="addNumber"
        android:tag="9" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Range: "
        android:id="@+id/rangeText"
        android:height="50dp"
        android:layout_below="@+id/button12"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/button12"
        android:layout_alignEnd="@+id/button12" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Entr"
        android:id="@+id/enterButton"
        android:height="70dp"
        android:onClick="submitNumber"
        android:layout_alignRight="@+id/congrats"
        android:layout_alignEnd="@+id/congrats"
        android:layout_toRightOf="@+id/button3"
        android:layout_toEndOf="@+id/button3"
        android:layout_alignBottom="@+id/button9"
        android:layout_below="@+id/button3" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Del"
        android:id="@+id/deleteButton"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/button3"
        android:layout_toEndOf="@+id/button3"
        android:height="70dp"
        android:width="70dp"
        android:onClick="deleteNumber"/>

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/numberOutput"
        android:layout_above="@+id/button"
        android:layout_alignRight="@+id/deleteButton"
        android:layout_alignEnd="@+id/deleteButton"
        android:layout_alignLeft="@+id/button2"
        android:layout_alignStart="@+id/button2"
        android:clickable="false"
        android:cursorVisible="false"
        android:focusable="false"
        android:focusableInTouchMode="false" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:id="@+id/congrats"
        android:layout_above="@+id/numberOutput"
        android:layout_alignRight="@+id/numberOutput"
        android:layout_alignEnd="@+id/numberOutput"
        android:layout_marginBottom="31dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:id="@+id/button11"
        android:width="70dp"
        android:minWidth="5dp"
        android:height="70dp"
        android:onClick="addNumber"
        android:tag="0"
        android:layout_below="@+id/button7"
        android:layout_toRightOf="@+id/button8"
        android:layout_toEndOf="@+id/button8" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Restart"
        android:id="@+id/button12"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/congrats"
        android:layout_alignEnd="@+id/congrats"
        android:height="70dp"
        android:layout_alignLeft="@+id/button3"
        android:layout_alignStart="@+id/button3"
        android:textSize="24dp"
        android:textAllCaps="false"
        android:onClick="restart"/>

</RelativeLayout>
